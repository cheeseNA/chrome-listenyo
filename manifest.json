{
   "manifest_version": 2,
   "name" : "chrome-listenyo",
   "version" : "0.0.0.1",
   "browser_action": {
      "default_title": "chrome-listenyo"
   },
   "background": {
        "scripts": [
        "background.js"
        ],
        "persistent":false
    },
   "permissions": [
      "tabs",
      "https://youtube.com/*",
      "activeTab"
   ]
}
